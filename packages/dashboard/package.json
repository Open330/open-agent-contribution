{
  "name": "@open330/oac-dashboard",
  "version": "2026.3.2",
  "description": "Web dashboard for monitoring OAC runs and contributions",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Open330/open-agent-contribution.git",
    "directory": "packages/dashboard"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsx src/dev.ts",
    "test": "vitest run --passWithNoTests",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@open330/oac-core": "workspace:*",
    "@open330/oac-repo": "workspace:*",
    "@open330/oac-discovery": "workspace:*",
    "@open330/oac-budget": "workspace:*",
    "@open330/oac-execution": "workspace:*",
    "@open330/oac-tracking": "workspace:*",
    "@fastify/cors": "^10.0.2",
    "@fastify/static": "^8.1.0",
    "@xterm/xterm": "^5.5.0",
    "execa": "^9.5.2",
    "fastify": "^5.2.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.15.1"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "tailwindcss": "^4.0.6",
    "tsx": "^4.19.0",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vite": "^6.1.0"
  }
}
