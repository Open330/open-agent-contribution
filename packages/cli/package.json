{
  "name": "@open330/oac",
  "version": "2026.3.3",
  "description": "CLI for Open Agent Contribution - automate open source contributions with AI agents",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Open330/open-agent-contribution.git",
    "directory": "packages/cli"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "oac": "./dist/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts --format esm --dts",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@open330/oac-core": "workspace:*",
    "@open330/oac-repo": "workspace:*",
    "@open330/oac-discovery": "workspace:*",
    "@open330/oac-budget": "workspace:*",
    "@open330/oac-execution": "workspace:*",
    "@open330/oac-completion": "workspace:*",
    "@open330/oac-tracking": "workspace:*",
    "@inquirer/prompts": "^6.0.0",
    "chalk": "^5.4.1",
    "cli-table3": "^0.6.5",
    "commander": "^13.1.0",
    "execa": "^9.5.2",
    "ora": "^8.1.1"
  },
  "devDependencies": {
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  }
}
