  "dependencies": {
    "@oac/core": "workspace:*",
    "@oac/repo": "workspace:*",
    "@oac/discovery": "workspace:*",
    "@oac/budget": "workspace:*",
    "@oac/execution": "workspace:*",
    "@oac/completion": "workspace:*",
    "@oac/tracking": "workspace:*",
    "@inquirer/prompts": "^6.0.0",
    "chalk": "^5.4.1",
    "cli-table3": "^0.6.5",
    "commander": "^13.1.0",
    "execa": "^9.5.2",
    "ora": "^8.1.1"
  },
  "devDependencies": {
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  }
}
